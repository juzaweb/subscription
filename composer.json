{
    "name": "juzaweb/subscription",
    "type": "juzaweb-plugin",
    "description": "Subscription payment support",
    "license": "MIT",
    "keywords": [
        "juzaweb",
        "subscription",
        "payment",
        "plugin"
    ],
    "extra": {
        "juzaweb": {
            "name": "Subscription",
            "description": "Subscription payment support",
            "domain": "subscription",
            "providers": [
                "Juzaweb\\Subscription\\Providers\\SubscriptionServiceProvider"
            ]
        },
        "laravel": {
            "providers": [
                "Juzaweb\\Subscription\\Providers\\AutoloadServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Subscription\\": "src/"
        },
        "files": ["helpers/helpers.php"]
    },
    "require": {
        "ext-openssl": "*",
        "juzaweb/paypal-rest-api-sdk-php": "^1.0",
        "srmklive/paypal": "~3.0"
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    }
}
